{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "exclude": [
        "node_modules"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-pascal-case"
        ],
        "object-literal-sort-keys": false,
        "semicolon": false
    },
    "rulesDirectory": []
}